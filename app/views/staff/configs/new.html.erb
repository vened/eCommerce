<%= form_for(@config, url: staff_configs_path) do |f| %>

    <section class="page-title">
        <h1>
            <%= t('configs.new') %>
        </h1>
        <div class="page-title-action">

            <button class="btn btn-sm btn-success">
                <i class="fa fa-check"></i>
                <%= t('save') %>
            </button>

        </div>
    </section>


    <%= render partial: 'form', locals: { f: f } %>

<% end %>
