<md-content class="md-padding">

    <uploader-component upload-url="<%= staff_api_v1_product_photo_products_path(@staff_product) %>"></uploader-component>


    <photos-upload photos="<%= @staff_product.photos.to_json %>"></photos-upload>


    <hr>
    <%- @staff_product.photos.each do |photo| %>
        <%= image_tag photo.file.thumb %>
    <% end %>

</md-content>
